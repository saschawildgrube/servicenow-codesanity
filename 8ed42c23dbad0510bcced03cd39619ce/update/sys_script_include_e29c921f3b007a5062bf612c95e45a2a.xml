    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_codesanity.IsCheckScriptCompliant</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns true if the given Check script is compliant and verifies if the check is really applicable to the current record.</description>
        <mobile_callable>false</mobile_callable>
        <name>IsCheckScriptCompliant</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function IsCheckScriptCompliant(strScript)
{
	var ScriptRemoveComments = x_snc_devtools.ScriptRemoveComments;
	var StringCheckRegEx = x_snc_devtools.StringCheckRegEx;

	strScript = ScriptRemoveComments(strScript);
			
	var strRegEx = '^\\s*\\(function\\s*\\(\\s*engine\\s*\\)\\s*\\{\\s*if\\s*\\(\\s*x_snc_codesanity.IsCheckApplicableToRecord\\('; // \\s*\\)\\)\\s*\\{';

	if (StringCheckRegEx(strScript,strRegEx) == false)
	{
		return false;
	}

	return true;
}]]></script>
